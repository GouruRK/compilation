
int putint_array(int array[], int size) {
    int i;
    i = 0;

    while (i < size) {
        putint(array[i]);
        i = i + 1;
    }
    return 0;
}

int main(void) {
    int array[5];
    
    array[0] = 1;
    array[1] = 2;
    array[2] = 3;
    array[3] = 4;
    array[4] = 5;
    return putint_array(array, 5);
}
